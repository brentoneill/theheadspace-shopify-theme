      background-color: #000;
      color: #fff;
      h4 {
        margin-bottom: 0;
      }
    }
}

.site-header__search {
  margin-bottom: 10px;
}

.search_form {
  margin-top: 12px;
}

.nav-item {
  &.dropdown {
    .dropdown-menu {
      border-radius: 0;
      box-shadow: 0 2px 4px -1px rgba(0,0,0,.2), 0 4px 5px 0 rgba(0,0,0,.14), 0 1px 10px 0 rgba(0,0,0,.12);
    }
    .list-title {
        line-height: 1.4;
        margin-bottom: 5px;
    }
  }
}

.main-header {
  z-index: 10;
  position: relative;
  box-shadow: 0 2px 4px -1px rgba(0,0,0,.2), 0 4px 5px 0 rgba(0,0,0,.14), 0 1px 10px 0 rgba(0,0,0,.12);
}

.main-header.affix {
  position: fixed;
  border-bottom: 1px solid #ddd;
  .main-header-inner .site-nav>li {
    line-height: 4;
    height: 100%;
  }
  .search_form {
    margin-top: 10px;
    margin-right: 0;
    padding-right: 0;
  }
}

.banner-home {
  width: 100%;
  min-height: 75vh;
  position: relative;
  text-align: center;
  padding: 80px;
  .text-wrapper {
    margin-top: 5vh;
    text-align: center;
    z-index: 1;
    position: relative;
  }

  .caption {
    display: table;
    clear: both;
    margin-left: auto;
    margin-bottom: 40px;
    width: 100%;
  }

  h1 {
    color: #000;
    font-size: 80px;
    line-height: 1;
    margin-bottom: 40px;
  }

  h2 {
    color: #000;
    font-size: 40px;
    line-height: 1.4;
    display: inline-block;
    width: 50%;
    padding: 20px;
    float: left;
  }
  .bg {
    z-index: 0;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-image: url('{{ "farrand.jpg" | asset_url }}');
    background-repeat: repeat;
  }
}

@media screen and (max-width: 768px) {
  .banner-home {
    padding: 10px;
    min-height: 25vh;
    h1 {
      font-size: 60px;
      margin-bottom: 20px;
      line-height: 1;
    }

    h2 {
      font-size: 36px;
      line-height: 1;
      padding: 10px;
      float: left;
    }
  }
}

@media screen and (max-width: 600px) {
  .banner-home {

    h1 {
      font-size: 48px;
    }

    h2 {
      font-size: 28px;
      display: block;
      width: 100%;
      text-align: center;
    }
  }
}
