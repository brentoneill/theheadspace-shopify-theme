/*----------Mixin----------*/
@mixin transition($var1, $var2, $var3) {
  transition: $var1 $var2 $var3;
  -webkit-transition: $var1 $var2 $var3;
  -moz-transition: $var1 $var2 $var3;
  -o-transition: $var1 $var2 $var3;
}
@mixin display_inline() {
  display: inline-block;
  *display: block;
  zoom: 1;
}
@mixin opacity($var1, $var2) {
  opacity: $var1;
  filter: alpha(opacity=$var2);
  -ms-filter:"progid:DXImageTransform.Microsoft.Alpha(Opacity=$var2)";
}
@mixin transform_scale($var1, $var2) {
  -webkit-transform: scale($var1, $var2);
  -moz-transform: scale($var1, $var2);
  -o-transform: scale($var1, $var2);
  transform: scale($var1, $var2);
}
@mixin transform_scale3d($var1, $var2, $var3) {
  webkit-transform: $var1 $var2 $var3;
  -moz-transform: $var1 $var2 $var3;
  -o-transform: $var1 $var2 $var3;
  -ms-transform: $var1 $var2 $var3;
  transform: $var1 $var2 $var3;
}
@mixin transform_rotate($var) {
  transform: rotate($var);
  -ms-transform: rotate($var);
  -webkit-transform: rotate($var);
}
@mixin transform_rotateY($var) {
  transform: rotateY($var);
  -moz-transform: rotateY($var);
  -webkit-transform: rotateY($var);
  -o-transform: rotateY($var);
}
@mixin transform_translateY($var) {
  -webkit-transform: translateY($var);
  -ms-transform: translateY($var);
  transform: translateY($var);
}
@mixin transform_translate($var1, $var2) {
  -webkit-transform: translate($var1, $var2);
  -ms-transform: translate($var1, $var2);
  transform: translate($var1, $var2);
}
@mixin transform_translate3d($var1, $var2, $var3) {
  -webkit-transform: translate3d($var1, $var2, $var3);
  -moz-transform: translate3d($var1, $var2, $var3);
  -ms-transform: translate3d($var1, $var2, $var3);
  -o-transform: translate3d($var1, $var2, $var3);
  transform: translate3d($var1, $var2, $var3);
}
@mixin animation($var1, $var2, $var3, $var4, $var5) {
  -webkit-animation: $var1 $var2 $var3 $var4 $var5;
  -moz-animation: $var1 $var2 $var3 $var4 $var5;
  -o-animation: $var1 $var2 $var3 $var4 $var5;
  animation: $var1 $var2 $var3 $var4 $var5;
}
@mixin transform_preserve() {
  transform-style: preserve-3d;
  -webkit-transform-style: preserve-3d;
  -moz-transform-style: preserve-3d;
  -o-transform-style: preserve-3d;
}
@mixin transform_perspective($var1, $var2, $var3){
  transform: $var1 $var2 $var3;
  -webkit-transform: $var1 $var2 $var3;
  -moz-transform: $var1 $var2 $var3;
  -ms-transform: $var1 $var2 $var3;
  -o-transform: $var1 $var2 $var3;
}
@mixin backface_visibility($var) {
  -moz-backface-visibility: $var;
  -webkit-backface-visibility: $var;
  backface-visibility: $var;
}
@mixin border_radius($var) {
  -moz-border-radius: $var;
  -webkit-border-radius: $var;
  border-radius: $var;
}
@mixin box-shadow($var1, $var2, $var3, $var4, $var5) {
  -webkit-box-shadow: $var1 $var2 $var3 $var4 $var5;  /* Safari 3-4, iOS 4.0.2 - 4.2, Android 2.3+ */
  -moz-box-shadow: $var1 $var2 $var3 $var4 $var5;  /* Firefox 3.5 - 3.6 */
  box-shadow: $var1 $var2 $var3 $var4 $var5;  /* Opera 10.5, IE 9, Firefox 4+, Chrome 6+, iOS 5 */
}
@mixin display_table() {
  display: table;
  table-layout: fixed;
}
@mixin display-flex($var) {
  display: -webkit-flex;
  display: -moz-flex;
  display: -ms-flex;
  display: flex;
  -webkit-flex-flow: row wrap;
  -moz-flex-flow: row wrap;
  -ms-flex-flow: row wrap;
  flex-flow: row wrap;
  justify-content: $var;
  -moz-justify-content: $var;
  -ms-justify-content: $var;
  -webkit-justify-content: $var;
}
@mixin smooth_font() {
  font-smooth: always;
  font-smoothing: antialiased;
  -moz-font-smoothing: antialiased;
  -webkit-font-smoothing: antialiased;
}
@mixin grayscale($var) {
  -webkit-filter: grayscale($var); /* Chrome, Safari, Opera */
  filter: grayscale($var);
}
@mixin placeholder($var){
  &::-webkit-input-placeholder {
    color: $var;
  }
  &:-moz-placeholder {
    color: $var;
  }
  &::-moz-placeholder {
    color: $var;
  }
  &:-ms-input-placeholder {
    color: $var;
  }
}
@mixin effect_banner() {
  position: absolute;
  content: '';
  top: 0;
  left: 0;
  width: 0;
  height: 100%;
  background-color: rgba(255,255,255,0.2);
  content: '';
  @include transition(ease-out, all, 0.6s);
  @include border_radius(5px);
  z-index: 1;
}
@mixin hover_effect_banner() {
  width: 100%;
}
/*placeholder*/
::-webkit-input-placeholder {
  @include placeholder($color_text_general);
}
:-moz-placeholder {
  @include placeholder($color_text_general);
}
::-moz-placeholder {
  @include placeholder($color_text_general);
}
:-ms-input-placeholder {
  @include placeholder($color_text_general);
}
input:focus::-webkit-input-placeholder {
  color: transparent !important;
  @include opacity(1,100);
}
input:focus:-moz-placeholder { /* Firefox 18- */
  color: transparent;
}
input:focus::-moz-placeholder {  /* Firefox 19+ */
  color: transparent;
}
input:focus:-ms-input-placeholder {
  color: transparent;
}
/*End placeholder*/

@mixin prefix($property, $value, $prefixes) {
  @each $prefix in $prefixes {
    @if $prefix == webkit {
      -webkit-#{$property}: $value;
    } @else if $prefix == moz {
      -moz-#{$property}: $value;
    } @else if $prefix == ms {
      -ms-#{$property}: $value;
    } @else if $prefix == o {
      -o-#{$property}: $value;
    } @else if $prefix == spec {
      #{$property}: $value;
    } @else  {
      @warn 'Unrecognized prefix: #{$prefix}';
    }
  }
}
@mixin gradient($side, $from, $to) {
  background: -ms-linear-gradient($side, $from 0%, $to 100%);
  background: linear-gradient(to $side, $from 0%, $to 100%);
}
@mixin display-flexbox() {
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  width: 100%; // necessary for ie10
}

@mixin flex-wrap($value: nowrap) {
  @include prefix(flex-wrap, $value, webkit moz ms spec);
}

@mixin flex-direction($value) {
  @include prefix(flex-direction, $value, webkit moz ms spec);
}
@mixin flex($value) {
  @include prefix(flex, $value, webkit moz ms spec);
}

@mixin flex-basis($width: auto) {
  // sass-lint:disable no-misspelled-properties
  -ms-flex-preferred-size: $width;
  @include prefix(flex-basis, $width, webkit moz spec);
}

@mixin align-self($align: auto) {
  // sass-lint:disable no-misspelled-properties
  -ms-flex-item-align: $align;
  @include prefix(align-self, $align, webkit spec);
}

@mixin justify-content($justify: flex-start) {
  @include prefix(justify-content, $justify, webkit ms spec);
}
@mixin align-items($value: stretch) {
  $alt-value: $value;

  @if $value == 'flex-start' {
    $alt-value: start;
  } @else if $value == 'flex-end' {
    $alt-value: end;
  }

  // sass-lint:disable no-misspelled-properties
  -ms-flex-align: $alt-value;
  @include prefix(align-items, $value, webkit moz ms o spec);
}

/*----------End Mixin----------*/

/*Global*/
.list--inline {
  li {
    @include display_inline();
    margin-bottom: 0;
    vertical-align: middle;
  }
}
.input-group {
  position: relative;
  display: table;
  width: 100%;
  border-collapse: separate;
}
.small--hide {
  @media(max-width: 767px) {
    display: none;
  }
}
@mixin visually-hidden() {
  // sass-lint:disable no-important
  position: absolute !important;
  overflow: hidden;
  clip: rect(0 0 0 0);
  height: 1px;
  width: 1px;
  margin: -1px;
  padding: 0;
  border: 0;
}
.visually-hidden,
.icon__fallback-text {
  @include visually-hidden();
}
.drawer {
  display: none;
}
.site-header__cart,
.site-header__search {
  position: relative;

  .icon-cart,
  .icon-search {
    vertical-align: middle;
  }
}
@media only screen and (min-width: 750px) {
  .site-header__cart {
    position: absolute;
    top: 50%;
    right: 0;
    padding: 8px 0;
    @include transform_translateY(-50%);
  }
}
.search-bar__submit,
.search-header__submit {
  @include display_inline();
  vertical-align: middle;
  position: absolute;
  left: 0;
  top: 0;
  padding: 0 0 0 5px;
  height: 100%;
  z-index: 1;
}
select {
  color: $color_text_general;
  line-height: 1.2;
  padding-right: 28px;
  text-indent: 0.01px;
  text-overflow: '';
  cursor: pointer;
  padding: 8px 15px;
}
input[type="search"] {
  -webkit-appearance: textfield;
  -webkit-box-sizing: content-box;
  -moz-box-sizing: content-box;
  box-sizing: content-box;
}
/*End Global*/


/*----------Header----------*/
$height_header: 90px;
$height_header_mobile: 30px;
$height_button: 36px;
.top-header {
  background: $bkg_top_header;
  height: $height_header;
  border-bottom: 1px solid $border_header;
  @media(max-width: 767px) {
    height: auto;
  }
  .top-header-wrapper{
    .top-header-inner{
      color: $color_text_header;
      line-height: $height_header;
      padding-left: 0;
      padding-right: 0;
      @media(max-width: 767px) {
        padding-left: $row_padding;
        padding-right: $row_padding;
        line-height: $height_header_mobile;
      }
      >div{
        @include display_inline();
        width: 50%;
        @media(max-width: 767px) {
          text-align: center;
          width: 100%;
        }
        a{
          color: $color_text_header;
        }
        &.left-area {
          text-align: left;
          @media(max-width: 767px) {
            text-align: center;
          }
        }
        &.right-area{
          text-align: right;
          float: right;
          @media(max-width: 767px) {
            text-align: center;
            float: none;
          }
          ul.customer_accounts{
            @include display_flex(right);
            float: right;
            position: relative;
            >li{
              padding: 0 $row_padding;
              height: $height_header;
              @media(max-width: 767px) {
                padding-left: 5px;
                padding-right: 5px;
                height: $height_header_mobile;
              }
              i.fa{
                padding-right: 5px;
                font-size: 13px;
              }
              &:last-child {
                padding-right: 0;
              }
              &.login-account {
                #loginBox {
                  min-width: 250px;
                  padding: 0;
                  margin-top: -30px;
                  @media(max-width: 767px) {
                    margin-top: 0;
                  }
                  #bodyBox{
                    @include display_inline();
                    >ul {
                      padding: 20px;
                      >li {
                        padding: 0;
                        width: 100%;
                        margin-bottom: 10px;
                        line-height: 1.55em;
                        &:last-child {
                          margin-bottom: 0;
                        }
                        a.btn, button {
                          color: $color_text_button;
                          background: $bkg_button;
                          border: 1px solid $color_border_button;
                          height: $height_button;
                          line-height: $height_button;
                          text-transform: uppercase;
                          &:hover {
                            color: $color_text_button_hover;
                            background: $bkg_button_hover;
                            border: 1px solid $color_border_button_hover;
                          }
                        }
                        label {
                          font-weight: $font_weight_thin;
                        }
                      }
                    }
                  }
                }
              }
              &:first-child {
                padding-left: 0;
              }
              @media(max-width: 767px) {
                span.currency_code{
                  &.visible-xs {
                    display: inline-block !important;
                    zoom: 1 !important;
                    *display: inline !important;
                  }
                }
              }
            }
          }
          .currency_group{
            @include display_inline();
            height: $height_header;
            padding-right: $row_padding;
            @media(max-width: 767px) {
              height: $height_header_mobile;
            }
            .currency{
              &.btn-group {
                height: $height_header;
                vertical-align: top;
                @media(max-width: 767px) {
                  height: $height_header_mobile;
                }
                i.fa{
                  padding-left: 5px;
                }
              }
            }
          }
        }
      }
    }
  }
}
$height_search: 36px;
$bkg_search: #fff;
$bkg_cart_count: #f4f647;
$height_menu: 70px;
$height_sticky_menu: 60px;
$height_header_mobile: 80px;
.main-header{
  background: $bkg_main_header;
  @include transition(ease, all, 0.5s);
  .main-header-inner{
    position: relative;
    .site-header{
      @include display_inline();
      width: 100%;
      height: $height_header;
      border-bottom: 1px solid $border_header;
      @media(max-width: 767px) {
        height: auto;
        position: relative;
      }
      >div {
        &.left-area {
          line-height: $height_header;
          text-align: left;
          padding-left: 0;
          @media(max-width: 767px) {
            padding-left: $row_padding;
            line-height: $height_header_mobile;
          }
        }
        &.middle-area {
          line-height: $height_header;
          text-align: center;
          @media(max-width: 767px) {
            line-height: $height_header_mobile;
          }
        }
        &.right-area{
          padding-top: 32px;
          padding-bottom: 32px;
          padding-right: 0;
          @media(max-width: 767px) {
            position: absolute;
            right: 0;
            top: 0;
            padding-right: $row_padding;
            padding-top: 16px;
            padding-bottom: 16px;
          }
          .right-area-group{
            @include display_inline();
            width: 100%;
            position: relative;
            .search_form {
              width: calc(100% - 60px);
              float: left;
              .site-header__search{
                form {
                  @include display_flex(left);
                  background: $bkg_search;
                  .collections-selector {
                    select {
                      color: $color_text_heading;
                      height: 52px;
                      line-height: 50px;
                      padding: 0 20px;
                      @include border_radius(0);
                      border: none;
                      border-right: 1px solid $border_general;
                      @media(max-width: 767px) {
                        padding: 0 5px;
                      }
                    }
                  }
                  input.search-header__input {
                    padding: 0 30px 0 15px;
                    height: $height_search;
                    line-height: $height_search;
                    background: $bkg_search;
                    @include border_radius(0);
                    box-shadow: none;
                    border: none;
                    width: calc(100% - 196px);
                  }
                  .search-header__submit {
                    position: absolute;
                    right: 15px;
                    left: auto;
                    top: 0;
                    border: none;
                    background: transparent;
                  }
                }
              }
            }
            .site-header__cart {
              text-align: center;
              background: url({{ 'icon_cart.png' | asset_url }}) no-repeat center 50%;
              background-color: $color_hover_general;
              width: 50px;
              height: 50px;
              @include border_radius(4px);
              float: right;
              display: block;
            }
          }
        }
      }
    }
    .bottom_header {
      @media(max-width: 767px) {
        display: none;
      }
    }
    .site-nav {
      @include display_inline();
      height: $height_menu;
      line-height: $height_menu;
      @include transition(ease, all, 0.5s);
      @media(max-width: 767px) {
        display: none;
      }
      >li {
        @include display_inline();
        vertical-align: middle;
        height: $height_menu;
        line-height: $height_menu;
        >a{
          font-size: $font_size_general;
          text-transform: uppercase;
          color: $color_text_menu;
          padding: 0 $row_padding;
          @include transition(ease, all, 0.5s);
          i.fa{
            padding-left: 5px;
          }
          @media (max-width: 991px) {
            padding: 0 8px;
            font-size: 12px;
          }
        }
        &:first-child {
          >a{
            padding-left: 0;
          }
        }
      }
    }
    .right_bottom_header{
      @include display_inline();
      float: right;
      vertical-align: middle;
      @include transition(ease, all, 0.5s);
    }
    .search_form {
      @include display_inline();
      padding: 0 $row_padding;
      position: relative;
      a{
        color: $color_text_menu;
      }
      form {
        @include display_inline();
        width: 100%;
        position: relative;
        input {
          display: table-cell;
          &.search-header__input {
            height: 40px;
            box-shadow: none;
            padding: 0 10px;
            width: 240px;
            border: 1px solid $border_general;
          }
        }
        .search-header__submit{
          position: absolute;
          right: 0;
          left: auto;
          border: none;
          box-shadow: none;
          margin-top: 10px;
          margin-right: 10px;
          width: 24px;
          height: 24px;
          background: transparent;
        }
        .collections-selector {
          @include display_inline();
          position: absolute;
          left: 0;
          margin-top: 22px;
          width: 30%;
          margin-left: 30px;
          #collection-option {
            width: 100%;
            height: $height_search;
            line-height: $height_search;
            font-weight: 600;
            padding-left: 15px;
            padding-right: 15px;
            border-color: $border_general;
            font-family: $font_general;
          }
        }
      }
    }
    #cart-target {
      @include display_inline();
      position: relative;
      > a {
        display: block;
        float: right;
        .num-items-in-cart {
          position: relative;
          i.fa {
            color: $color_text_header;
          }
          .block-cart {
            text-align: center;
            background: url({{ 'icon_cart.png' | asset_url }}) no-repeat center 50%;
            background-color: $color_text_menu;
            width: 40px;
            height: 40px;
            @include border_radius(0);
            span.icon {
              position: absolute;
              top: -10px;
              right: 0;
              left: 0;
              vertical-align: top;
              background-color: $color_text_menu;
              width: 20px;
              height: 20px;
              border-radius: 50%;
              margin: 0 auto;
              text-align: center;
              span.number {
                position: absolute;
                height: 20px;
                line-height: 20px;
                color: #fff;
                text-align: center;
                display: block;
                width: 100%;
              }
            }
          }
        }
      }
      #cart-info {
        left: auto;
        right: 0;
        width: 320px;
        height: auto;
        margin: 0;
        padding: 20px;
        line-height: 15px;
        border-radius: 0;
        @media(max-width: 320px) {
          width: 291px;
        }
        #cart-content {
          line-height: 15px;
          .items {
            position: relative;
            display: inline-block;
            width: 100%;
            line-height: 10px;
            vertical-align: top;
            .row {
              position: relative;
              margin: 20px 0;
              padding-bottom: 20px;
              border-bottom: 1px solid $border_submenu;
              a.cart-close {
                @include display-inline();
                text-align: left;
