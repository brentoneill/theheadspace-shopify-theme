@media(max-width: 767px) {
  #cart {
    #cartform {
      table {
        thead {
          display: none;
        }
        td {
          width: 100%;
          float: left;
          display: block;
          height: auto;
          padding-left: 120px;
          padding-top: 0;
          padding-bottom: 0;
          position: relative;
          text-align: left;
          min-height: 70px;
          &:before {
            height: 100%;
            background-color: $shop_bg_color;
            content: '';
            position: absolute;
            left: 0;
            width: 100px;
            overflow: hidden;
          }
          &:after {
            height: 20px;
            top: 50%;
            margin-top: -10px;
            line-height: 20px;
            text-align: center;
            font-weight: bold;
            text-transform: uppercase;
            position: absolute;
            left: 0;
            width: 100px;
            overflow: hidden;
          }
          &.image {
            .product_image {
              margin-top: 20px;
              margin-bottom: 20px;
              display: block;
              width: 100%;
            }
            .product_name {
              display: block;
              width: 100%;
              padding: 0;
            }
            &:after {
              content: 'Images';
            }
          }
          &.price {
            &:after {
              content: 'Price';
            }
            span.money {
              margin-top: 20px;
              display: block;
            }
          }
          &.qty {
            .quantity-wrapper {
              margin-top: 20px;
              display: block;
            }
            &:after {
              content: 'Quantity';
            }
          }
          &.total {
            &:after {
              content: 'Total';
            }
            span.money {
              margin-top: 20px;
              display: block;
            }
          }
          &.remove {
            &:after {
              content: 'Remove';
            }
            a.cart {
              margin-top: 20px;
              display: block;
            }
          }
        }
        tr.summary {
          td.total_text {
            display: none;
          }
        }
      }
    }
  }
}
